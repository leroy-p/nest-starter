{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "env:cmd": "env-cmd -f .env",
    "docker:development": "docker-compose --file ./docker-compose.yml --env-file .env",
    "build:graphql": "graphql-codegen --config graphql-codegen.yml",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:merge-schema": "yarn run prisma-merge -b \"./src/modules/prisma/base.prisma\" -e \"./src/modules/prisma/schema.prisma\" -o \"./src/modules/prisma/schema.prisma\" -s \"./src/modules/**/*.prisma\"",
    "prisma:db-push": "yarn run prisma:merge-schema && yarn run env:cmd prisma db push",
    "prisma:reset": "yarn run env:cmd prisma migrate reset --force && yarn run prisma:db-push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "10.0.11",
    "@nestjs/common": "8.1.1",
    "@nestjs/core": "8.1.1",
    "@nestjs/graphql": "10.0.11",
    "@nestjs/platform-express": "8.1.1",
    "@prisma/client": "3.9.1",
    "apollo-server-express": "3.7.0",
    "graphql": "16.5.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-graphql-request": "4.3.7",
    "@graphql-tools/merge": "8.2.6",
    "@graphql-tools/schema": "8.3.6",
    "@graphql-tools/utils": "8.6.5",
    "graphql": "16.5.0",
    "type-graphql": "1.1.1",
    "@nestjs/cli": "8.1.3",
    "@nestjs/schematics": "8.0.4",
    "@nestjs/testing": "8.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "env-cmd": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "esm": "3.2.25",
    "jest": "27.3.0",
    "lerna": "4.0.0",
    "prisma": "3.9.1",
    "prettier": "2.4.1",
    "prisma-merge": "0.2.0",
    "source-map-support": "0.5.20",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "prisma": {
    "schema": "src/modules/prisma/schema.prisma"
  }
}
